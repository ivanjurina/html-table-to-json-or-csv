{
    "manifest_version": 3,
    "name": "Table to CSV/JSON",
    "version": "1.0",
    "author": "ivanjurina@ivanjurina.com",
    "permissions": [
      "activeTab",
      "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ]
  }
  